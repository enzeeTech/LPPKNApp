{
  "expo": {
    "name": "LPPKN On The Go",
    "slug": "lppknApp",
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./app/assets/appIconIOS.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "my.gov.lppkn.go",
      "icon": "./app/assets/appIconIOS.png",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to show nearby clinics and services available to you."
      },
      "buildNumber": "1"
    },
    "android": {
      "package": "com.socio.lppknApp",
      "softwareKeyboardLayoutMode": "pan",
      "googleServicesFile": "./google-services.json",
      "versionCode": 12
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8d7a4c07-5171-4e06-9cd0-0d2899e2ee47"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/8d7a4c07-5171-4e06-9cd0-0d2899e2ee47"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}
